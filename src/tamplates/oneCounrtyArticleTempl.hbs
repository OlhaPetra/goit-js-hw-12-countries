{{#each this}}
<li class="card">
<h2 class="card__name">{{name}}</h2>
<p class="card__capital">Capital: {{capital}}</p>
<p class="card__population">Population: {{population}}</p>
<p class="card__population">Languages:</p>
        <ul class="card__languages tag-list">
            
            {{#each languages}}
            <li class="tag-list__item">{{name}}</li>
            {{/each}}
        </ul>
<img class="card__flag" src="{{flag}}" alt="{{name}} flag">

{{/each}}